   "source": [
    "def flatten(items):\n",
    "    for x in items:\n",
    "        if isinstance(x, list) :\n",
    "            yield from flatten(x)\n",
    "        else:\n",
    "            yield x\n",
    " \n",
    "l = [[1,'a',['cat'],2],[[[3]],'dog'],4,5]\n",
    "\n",
    "print(list(flatten(l)))"
   ]
  }
